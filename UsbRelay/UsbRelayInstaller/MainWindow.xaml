<Window x:Class="UsbRelayInstaller.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UsbRelayInstaller"
        mc:Ignorable="d"
        Title="Install USB Relay Control" Height="200" Width="450">
    <Grid>
        <ProgressBar Name="progress" HorizontalAlignment="Center" Height="20" Margin="0,123,0,0" VerticalAlignment="Top" Width="400" ValueChanged="progress_ValueChanged"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Center" Margin="0,46,0,0" Text="Installing..." TextWrapping="Wrap" VerticalAlignment="Top" FontSize="32"/>

    </Grid>
</Window>
